<html>
<head>
    <% include ./include/header.ejs %>
</head>
<body>
<div class="wrapper">
    <div class="parallax">
        <!--<div class="parallax-image">-->
        <!--<img src="static/img/hyundai.png">-->
        <!--</div>-->
        <div class="motto">
            <div class="form-group has-success">
                <input id="searchText" autocomplete="off" type="text" placeholder="검색어를 입력하세요" class="form-control" />
            </div>
        </div>
        <h1 style="text-align: center; margin-top:80px;">제주한라대학교</h1>
        <h3 class="motivation">향토자료관</h3>
    </div>
</div>
<% include ./include/nav.ejs %>

<div id="section" class="facets">
    <div id="content" style="padding-left: 10px; padding-right: 10px;">
        <div class="container-fluid">
            <div class="row">
                <!-- left facets -->
                <div class="col-md-3" id="left_facets"></div>
                <!-- body list -->
                <div class="col-md-9" id="body_data"></div>
            </div>
            <!-- page 부분 -->
            <div class="row">
                <div class="col-md-offset-3 col-md-9">
                    <nav aria-label="Page navigation">
                        <div class="pages_wrap">
                            <ul class="pagination">
                                <li class="page-item">
                                    <a class="page-link" href="#" aria-label="Previous">
                                        <span aria-hidden="true">&lt;</span>
                                        <span class="sr-only">Previous</span>
                                    </a>
                                </li>
                            </ul>
                            <ul class="pagination page_wrap"></ul>
                            <ul class="pagination">
                                <li class="page-item">
                                    <a class="page-link" href="#" aria-label="Next">
                                        <span aria-hidden="true">&gt;</span>
                                        <span class="sr-only">Next</span>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </nav>
                </div>
            </div>
            <!-- page 부분 끝 -->
        </div>
    </div>
</div>
<div style="display:none">
    <div id="data_store" data-store="<%=store%>"></div>
    <div id="data_left" data-store="<%=left_store%>"></div>
    <div id="left" data-store="<%=left%>"></div>
    <div id="left_menu" data-store="<%=left_menu%>"></div>
    <input type="hidden" name="now_page" value="1"/>
    <input type="hidden" name="now_group" value="1"/>
</div>
</body>
<script src="static/js/custom.js"></script>
<% include ./include/footer.ejs %>
</html>
