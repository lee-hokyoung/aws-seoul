<html>
<head>
    <% include ./include/header.ejs %>
    <script src="../static/amcharts/amcharts/amcharts.js"></script>
    <script src="../static/amcharts/amcharts/serial.js"></script>
    <script src="../static/amcharts/amcharts/themes/light.js"></script>
</head>
<body>
<% include ./include/nav.ejs %>
<style>
    #chartdiv {
        width		: 100%;
        height		: 350px;
        font-size	: 11px;
    }
</style>
<div id="section" class="facets wide-t125">
    <div id="content" style="padding-left: 10px; padding-right: 10px;">
        <div class="container">
            <div class="row"><div id="timeWrap"></div></div>
            <div class="row">
                <div class="col-md-2">
                    <div id="chart_left" class="list-group">
                        <a type="button" class="list-group-item list-group-item-action">기록물건명</a>
                        <a type="button" class="list-group-item list-group-item-action">대외자료</a>
                        <a type="button" class="list-group-item list-group-item-action">이벤트</a>
                    </div>
                </div>
                <div class="col-md-9"><div id="chartdiv"></div></div>
            </div>
            <div class="row">
                <div class="col-md-3" id="left_facets"></div>
                <div class="col-md-9">
                    <input id="timeline1" width="500px" type="hidden" class="span2" value="" data-slider-step="1"/>
                    <div id="body_data"></div>
                </div>
                <!-- body list -->
                <!-- timeline scroll  -->
            </div>
            <!-- page 부분 -->
            <div class="row" style="display: none;">
                <div class="col-md-offset-3 col-md-9">
                    <nav aria-label="Page navigation">
                        <div class="pages_wrap">
                            <ul class="pagination">
                                <li class="page-item">
                                    <a class="page-link" href="#" aria-label="Previous">
                                        <span aria-hidden="true">&lt;</span>
                                        <span class="sr-only">Previous</span>
                                    </a>
                                </li>
                            </ul>
                            <ul class="pagination page_wrap"></ul>
                            <ul class="pagination">
                                <li class="page-item">
                                    <a class="page-link" href="#" aria-label="Next">
                                        <span aria-hidden="true">&gt;</span>
                                        <span class="sr-only">Next</span>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </nav>
                </div>
            </div>
            <!-- page 부분 끝 -->
        </div>
    </div>
</div>
<div style="display:none">
    <div id="body_store" data-store="<%=body%>"></div>
    <div id="data_left" data-store="<%=left_store%>"></div>
    <div id="left_store" data-store="<%=left%>"></div>
    <div id="left_menu" data-store="<%=left_menu%>"></div>
    <input type="hidden" name="now_page" value="1"/>
    <input type="hidden" name="now_group" value="1"/>
</div>
</body>
<% include ./include/footer.ejs %>
<script src="static/bootstrap3/js/bootstrap-slider.min.js"></script>
<script src="static/js/timeline.js"></script>
</html>
